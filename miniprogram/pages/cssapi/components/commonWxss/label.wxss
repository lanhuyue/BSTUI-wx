@import './variable.wxss';
.BTS-label-default{
    line-height: 30rpx;
    padding: 0 6rpx;
    color: #fff;
    font-size: var(--fontSize11);
    font-weight: 400;
    border-radius: 4rpx;
    text-align: center;
    display: inline-block;
}
/*label样式1 */
.BTS-label-default-bg1{
    background:var(--chemeColorBrand);
}
.BTS-label-default-bg2{
    background:var(--auxiliaryColor);
}
.BTS-label-default-bg3{
    background:var(--secondaryColorGreen);
}
.BTS-label-default-bg4{
    background:var(--secondaryColorRed);
}
.BTS-label-default-bg5{
    background:var(--textDarkColor-100);
}
/* label样式2 */
.BTS-label-box1{
    border: 2rpx solid var(--chemeColorBrand);
    color: var(--chemeColorBrand);
}
.BTS-label-box2{
    border: 2rpx solid var(--auxiliaryColor);
    color: var(--auxiliaryColor);
}
.BTS-label-box3{
    border: 2rpx solid var(--secondaryColorGreen);
    color: var(--secondaryColorGreen);
}
.BTS-label-box4{
    border: 2rpx solid var(--secondaryColorRed);
    color: var(--secondaryColorRed);
}
.BTS-label-box5{
    border: 2rpx solid var(--textDarkColor-100);
    color: var(--textDarkColor-100);
}
/* label样式3 */
.BTS-label-opacity1{
    background: rgba(79,164,249,0.2);
    color: var(--chemeColorBrand);
}
.BTS-label-opacity2{
    background: rgba(244,136,49,0.2);
    color: var(--auxiliaryColor);
}
.BTS-label-opacity3{
    background: rgba(17,199,167,0.2);
    color: var(--secondaryColorGreen);
}
.BTS-label-opacity4{
    background: rgba(255,133,133,0.2);
    color: var(--secondaryColorRed);
}
.BTS-label-opacity5{
    background: rgba(191,191,191,0.2);
    color: var(--textDarkColor-100);
}
/* labe样式-其他 */
.BTS-label-red{
    display: inline-block;
    line-height: 32rpx;
    background:rgba(255,82,93,1);
    border-radius: 16rpx;
    padding: 0 8px;
    font-weight:500;
    color:rgba(255,255,255,1);
    font-size:var(--fontSize10);
}
.BTS-label-choose{
    color:var(--chemeColorBrand);
    display: inline-block;
    background:rgba(73, 164, 249,0.2);
    border-radius:4px;
    line-height: 68rpx;
    padding: 0 32rpx;
    font-weight:400;
    font-size: 28rpx;
}
.BTS-label-choose.active{
    background:var(--chemeColorBrand)!important;
    color:var(--textDarkColor-200)!important;
}
.BTS-label-beveling{
    display: inline-block;
    line-height:32rpx;
    color:rgba(255,255,255,1);
    font-size:var(--fontSize11);
    padding: 0 32rpx 0 16rpx;
    text-align: center;
}
